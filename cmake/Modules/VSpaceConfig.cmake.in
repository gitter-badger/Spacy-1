# - Config file for the VSpace package
# It defines the following variables
#  VSPACE_INCLUDE_DIRS - include directories for VSpace
#  VSPACE_LIBRARIES    - libraries to link against
#  VSPACE_EXECUTABLE   - the bar executable
 
# Compute paths
get_filename_component(VSPACE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(VSPACE_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# These are IMPORTED targets created by VSpaceTargets.cmake
set(VSPACE_LIBRARY_DIRS "@CONF_LIBRARY_DIRS@")
set(VSPACE_LIBRARIES vspace)


# Set compiler flags
set(VSPACE_CXX_FLAGS "${CMAKE_CXX_FLAGS} -std=c++1y")